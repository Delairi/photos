{
  "version": 1,
  "paths": {
    "/auth/login": {
      "name": "/auth/login",
      "lambdaFunction": "Login",
      "permissions": {
        "setting": "open"
      }
    },
    "/auth/verify": {
      "name": "/auth/verify",
      "lambdaFunction": "VerifyAccount",
      "permissions": {
        "setting": "open"
      }
    },
    "/auth/signup": {
      "name": "/auth/signup",
      "lambdaFunction": "Signup",
      "permissions": {
        "setting": "open"
      }
    },
    "/files/presign": {
      "name": "/files/presign",
      "lambdaFunction": "Presign",
      "permissions": {
        "setting": "private",
        "auth": [
          "create",
          "read",
          "update"
        ]
      }
    },
    "/albums": {
      "name": "/albums",
      "lambdaFunction": "CreateAlbum",
      "permissions": {
        "setting": "open"
      }
    }
  }
}